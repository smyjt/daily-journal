<%- include("partials/header") -%>

<h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="postTitle" required autocomplete="off">
      <label>Post</label>
      <textarea class="form-control" name="postBody" rows="6" cols="60" required autocomplete="off"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
  </form>

<%- include("partials/footer") -%>
